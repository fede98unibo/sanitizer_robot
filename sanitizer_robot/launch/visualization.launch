<launch>
  <!-- Launch the grid map simple demo node -->
  <node pkg="sanitizer_robot" type="uv_visualization" name="uv_visualization" output="screen" />
  <!-- Launch the grid map visualizer -->
  <node pkg="grid_map_visualization" type="grid_map_visualization" name="grid_map_visualization" output="screen">
    <rosparam command="load" file="$(find sanitizer_robot)/config/uv_visualization_params.yaml" />
  </node>
</launch>